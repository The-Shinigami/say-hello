pipeline {
    agent any
    triggers {
        pollSCM '* * * * *'
    }
    stages{
      stage('Build') {
            steps {
                sh 'mvn clean pakcage'
            }
        }

      stage('deploy') {
            steps {
                sh 'java -jar target/say-hello-0.0.1.jar'
            }
        }
    }        
    
}
